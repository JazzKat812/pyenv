#require_gcc
prefer_openssl11
export PYTHON_BUILD_CONFIGURE_WITH_OPENSSL=1
install_package "openssl-1.1.0j" "https://www.openssl.org/source/old/1.1.0/openssl-1.1.0j.tar.gz#31bec6c203ce1a8e93d5994f4ed304c63ccf07676118b6634edded12ad1b3246" mac_openssl --if has_broken_mac_openssl
install_package "readline-8.0" "https://ftpmirror.gnu.org/readline/readline-8.0.tar.gz#e339f51971478d369f8a053a330a190781acb9864cf4c541060f12078948e461" mac_readline --if has_broken_mac_readline
if has_tar_xz_support; then
    install_package "Python-3.10.0a1" "https://www.python.org/ftp/python/3.10.0/Python-3.10.0a1.tar.xz#db739461233fc1c0c15ccf4e35455bedcc7524a086935e11404df9c05352a960" ldflags_dirs standard verify_py39 copy_python_gdb ensurepip
else
    install_package "Python-3.10.0a1" "https://www.python.org/ftp/python/3.10.0/Python-3.10.0a1.tgz#e69048f4438cdd321eff1541c7d12eb92a843041419c59ba05c3e55c967373f7" ldflags_dirs standard verify_py39 copy_python_gdb ensurepip
fi
